
@{
    ViewData["Title"] = "_UserLayout";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebsiteProject</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>
    <div class="container">
        <ul>
            <a target="_self" href="/">
                <img class="logo" src="~/images/logo.png">
            </a>
            <li><a href="/cameras" style="font-size: 34px">Kamery</a></li>
            <li><a href="/users" style="font-size: 34px">Użytkownicy</a></li>
        </ul>
    </div>
    <div class="container" style="background-color:lightgreen">
        @RenderBody()
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

